    <div class="roombar">
        <div class="room_area">
            <p class="room_title_noi">exploper</p>
            <p class="room_title">
                <i class="fas fa-chevron-right"></i> open editors
            </p>
            <p class="room_title chosse"><i class="fas fa-chevron-down"></i> online user</p>
