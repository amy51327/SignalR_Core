    @*聊天畫面*@
    <div class="right-block">
        @*表頭頁籤*@
        <div class="chat_bar">
            <div class="chat_tab">
                <img src="~/images/csharp-icon.png" />
                ILoveMyJob.cs  <span class="closeButton">x</span>
            </div>
        </div>
        <div id="chat_content">
            <div id="messagesList"></div>
        </div>

        <div class="msg_input_area">
            <div class="msg_tab">
                <div class="active">TERMINAL</div>
                <div>PROBLEMS</div>
                <div>OUTPUT</div>
                <div>DEBUG CONSOLE</div>
                <div class="msg_input_area_right">
                    <i class="fas fa-user "></i>
                </div>
            </div>

            <div class="commment">
                <p>Windows PowerShell</p>
                <p>Copyright (C) Amie Chen.</p>
            </div>
            <div class="msg_input">
                <p> PS C:\Side_Project\<span id="userInput" value=""></span>></p>
                <input type="text" id="messageInput" />
            </div>
        </div>
    </div>
    <div class="footer">
        <div class="footer-left">
            <div>
